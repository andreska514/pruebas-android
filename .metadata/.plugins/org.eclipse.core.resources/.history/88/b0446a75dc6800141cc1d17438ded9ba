package com.andres.sun4all;

import java.io.IOException;
import java.net.MalformedURLException;
import java.net.URL;

import android.graphics.Bitmap;
import android.graphics.BitmapFactory;
import android.os.AsyncTask;

public class Download extends AsyncTask<String, String, String>{

	Bitmap b;
    @Override
    protected String doInBackground(String... aurl) {
    	String name = Main.imgBase.concat("k1v_01_07_00_09h_35.jpg");
		try {
			URL url_value = new URL(name);
			b = BitmapFactory.decodeStream(url_value.openConnection().getInputStream());
		} catch (IOException e) {}
		return null;
    }
    Bitmap downloadUrl(String s) throws IOException{
		String name = Main.imgBase.concat(s);
		URL url_value = new URL(name);
		Bitmap mIcon1 = BitmapFactory.decodeStream(url_value.openConnection().getInputStream());
		return mIcon1;
		
	}
}

